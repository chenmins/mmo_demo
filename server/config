root = "./"
thread = 8
logger = nil
harbor = 0
start = "main"  -- 启动 main.lua
bootstrap = "snlua bootstrap"

-- 路径配置
luaservice = root.."service/?.lua;"..root.."skynet/service/?.lua"
lualib = root.."lualib/?.lua;"..root.."skynet/lualib/?.lua"
-- 注意：这里假设你把 skynet 源码放在了 server/skynet 目录下，或者你需要修改路径指向你的 skynet 安装目录
-- 另外加入了 luaclib 目录以便加载 aoi.so
lua_cpath = root.."luaclib/?.so;"..root.."skynet/luaclib/?.so"
